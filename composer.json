{
    "name": "thinktomorrow/chief-sitestructure",
    "description": "Site structure and breadcrumb assistant for Thinktomorrow/chief cms",
    "type": "library",
    "keywords": ["laravel", "chief", "chief-cms", "site structure", "breadcrumbs"],
    "require": {
        "thinktomorrow/chief": "^0.4.8",
        "thinktomorrow/vine": "^0.2.6"
    },
    "require-dev": {
        "orchestra/testbench": "3.8.*|4.*",
        "phpunit/phpunit": "~7.5|~8.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Philippe Damen",
            "email": "philippe@thinktomorrow.be"
        }
    ],
    "autoload": {
        "psr-4": {
            "Thinktomorrow\\ChiefSitestructure\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Thinktomorrow\\ChiefSitestructure\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Thinktomorrow\\ChiefSitestructure\\ChiefSitestructureServiceProvider"
            ]
        }
    }
}
